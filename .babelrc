{
  "plugins": [
    [
      "dotenv-import",
      {
        "moduleName": "@env",
        "path": ".env",
        "blacklist": null,
        "whitelist": null,
        "safe": false,
        "allowUndefined": false
      }
    ]
  ],
  "presets": ["module:metro-react-native-babel-preset", "module:react-native-dotenv"]
}